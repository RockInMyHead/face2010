[run]
source = .
omit = 
    tests/*
    */__pycache__/*
    */site-packages/*
    test_*.py
    debug_*.py
    simple_test.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
